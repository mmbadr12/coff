<div class="container" >
    
    <form action="{{#}}" method="POST" enctype="multipart/form-data" class="form-group mt-5" >

        <a class="signup" href="{{#}}">Sing Up</a>

        <label>

            <input type="text" name="username" placeholder="Username" id="gix">
            <i id="user" class="fa fa-user fa-lg fa-fw" for="gix"></i>
       
        </label>
       
        <label>

            <input type="password" name="pass" placeholder="Password" id="fix">
            <i id="key" class="fa fa-lock fa-lg fa-fw" for="fix"></i>

        </label>

        <input type="submit" name="submit" class="btn btn-outline-info" value="LOGIN">
       

        <a class="forgot" href="{{#}}">Forgot Password</a>
